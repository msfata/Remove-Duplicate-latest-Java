
import java.util.ArrayList;
import java.util.Arrays;
import java.util.LinkedHashSet;

public class DuplicateRemover {
	public static void main(String[] args) {

		ArrayList<Integer> list = new ArrayList<Integer>(Arrays.asList(1, 3, 2, 2, 1, 3));

		LinkedHashSet<Integer> set = new LinkedHashSet<Integer>(list);

		ArrayList<Integer> l = new ArrayList<Integer>(set);

		l.forEach(v -> System.out.println(v));
	}
}
